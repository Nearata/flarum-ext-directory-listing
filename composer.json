{
    "name": "nearata/flarum-ext-directory-listing",
    "description": "Exposes the contents of a folder.",
    "keywords": [
        "flarum"
    ],
    "type": "flarum-extension",
    "license": "Unlicense",
    "require": {
        "flarum/core": "^1.6.0"
    },
    "authors": [
        {
            "name": "Nearata",
            "email": "nearata@protonmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Nearata\\DirectoryListing\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "Directory Listing",
            "category": "feature",
            "icon": {
                "name": "fas fa-folder",
